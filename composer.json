{
  "name": "starfishplatform/core",
  "type": "library",
  "description": "Starfish core Library",
  "license": "MIT",
  "keywords": ["starfish", "fishead", "connector"],
  "homepage": "https://github.com/wizardshankar/fishead_core.git",
  "authors": [
    {
      "name": "Naga",
      "email": "naga@fishead.com.au"
    },
    {
      "name": "Starfishplatform",
      "homepage": "https://github.com/wizardshankar/fishead_core.git/contributors"
    }
  ],
  "require": {
    "php": "^5.5.9|>=7.0.8",
    "symfony/http-foundation": "~3.0|~4.0",
    "alcaeus/mongo-php-adapter": "^1.1",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "doctrine/migrations": "^1.5",
    "doctrine/mongodb": "^1.6",
    "doctrine/mongodb-odm": "^1.2",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "doctrine/orm": "^2.5",
    "friendsofsymfony/user-bundle": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": { "SFP\\": "" },
    "exclude-from-classmap": ["/Tests/"]
  },
  "autoload-dev": {
    "psr-4": { "Starfish\\Tests\\": "tests/" }
  }
}